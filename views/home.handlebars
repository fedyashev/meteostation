<div class="panel panel-default">
  <table class="table table-hover">
    <thead>
      <tr>
        <th>Parameter</th>
        <th>Value</th>
      </tr>
    </thead>
    <tbody>
      {{#each parameters}}
        <tr>
          <td>{{name}}</td>
          <td>{{value}}</td>
        </tr>
      {{/each}}
    </tbody>
  </table>  
</div>
