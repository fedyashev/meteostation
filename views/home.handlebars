<link type="text/css" rel="StyleSheet" href="/stylesheets/chart.css"/>
<script src="/javascripts/chart.js"></script>

<div class="row">
  <div class="col-sx-6 col-md-6">
    <div class="panel panel-default">
      <table class="table table-hover">
        <thead>
          <tr>
            <th>Parameter</th>
            <th>Value</th>
          </tr>
        </thead>
        <tbody>
          {{#each parameters}}
            <tr>
              <td>{{name}}</td>
              <td>{{value}}</td>
            </tr>
          {{/each}}
        </tbody>
      </table>  
    </div>
  </div>
  <div class="col-sx-6 col-md-6">
<!--  -->

<!-- График --><div id="chart"></div>

<script>
  $(function () {
    $("#chart").shieldChart({
      theme: "light",
      primaryHeader: {
        text: "{{multiChart.mutliChartHeader}}"
      },
      exportOptions: {
        image: false,
        print: false
      },
      axisX: {
        title: {
          text: "Время, ч"
        },
        categoricalValues: {{{multiChart.dataX}}}
      },
      axisY: [{
        //min: 1,
        //max: 60,
        title: {
          text: "{{multiChart.temperatureChart.scaleName}}",
          style: {
            color: '#4DB0F5'
          }
        },
        axisTickText: {
          style: {
            color: '#4DB0F5'
          }
        }
      }, {
        //min: 1,
        //max: 5,
        title: {
          text: "{{multiChart.humidityChart.scaleName}}",
          style: {
            color: '#FFC500'
          }
        },
        swapLocation: true,
        axisTickText: {
          style: {
            color: '#FFC500'
          }
        }
      }, {
        //min: 1,
        //max: 40,
        title: {
          text: "{{multiChart.pressureChart.scaleName}}",
          style: {
            color: '#FF1800'
          }
        },
        swapLocation: true,
        axisTickText: {
          style: {
            color: '#FF1800'
          }
        }
      }],
      dataSeries: [{
        seriesType: 'line',
        axis: 0,
        collectionAlias: "{{multiChart.temperatureChart.curveName}}",
        data: {{{multiChart.temperatureChart.data}}}
      }, {
        seriesType: 'line',
        axisY: 1,
        collectionAlias: "{{multiChart.humidityChart.curveName}}",
        data: {{{multiChart.humidityChart.data}}}
      }, {
        seriesType: 'line',
        axisY: 2,
        collectionAlias: "{{multiChart.pressureChart.curveName}}",
        data: {{{multiChart.pressureChart.data}}}
      }]
    });
  });
</script><!-- /.График -->

<!--  -->
  </div>
</div>